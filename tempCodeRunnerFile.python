m = int(input("Enter the lower bound (m): "))
n = int(input("Enter the upper bound (n): "))

if m > n:
    m, n = n, m  

print(f"Prime numbers between {m} and {n}:")
for num in range(m, n + 1):
    if num > 1:
        is_prime = True
        for i in range(2, int(num ** 0.5) + 1):
            if num % i == 0:
                is_prime = False
                break
        if is_prime:
            print(num, end=" ")
